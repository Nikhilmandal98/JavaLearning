package homework;

import java.util.Collections;
import java.util.Stack;

public class StackDemo {

	public static void main(String[] args) {

Stack<String> nikhil = new Stack<String>();
nikhil.add("Biju");
nikhil.add("Nipu");
nikhil.add("Mim");
nikhil.add("Jon");
nikhil.add("Ron");
nikhil.add("Popy");
nikhil.add("Ron");
nikhil.pop();
nikhil.peek();
Collections.sort(nikhil,Collections.reverseOrder());
System.out.println(nikhil);
System.out.println(nikhil.empty());
System.out.println(nikhil.peek());
System.out.println(nikhil.pop());

System.out.println(nikhil);

	}
}

